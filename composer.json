{
    "name": "interworks/thoughtspotrest",
    "description": "This package allows simple REST API communication with your ThoughtSpot cluster.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "InterWorks\\ThoughtSpotREST\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InterWorks\\ThoughtSpotREST\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InterWorks\\ThoughtSpotREST\\ThoughtSpotRESTServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Justin Lyons",
            "email": "justin.lyons@interworks.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "pestphp/pest": "^3.5",
        "orchestra/testbench": "^9.5",
        "phpstan/phpstan": "^1.12"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
